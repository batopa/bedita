{
    "name": "bedita/bedita",
    "description": "BEdita the Semantic Content Management Framework",
    "homepage": "http://bedita.com",
    "type": "project",
    "license": "LGPL",
    "require": {
        "php": ">=5.4.19",
        "cakephp/cakephp": "3.0.*-dev",
        "mobiledetect/mobiledetectlib": "2.*"
    },
    "require-dev": {
        "d11wtq/boris": "1.0.*"
    },
    "suggest": {
        "phpunit/phpunit": "Allows automated tests to be run without system-wide install."
    },
    "autoload": {
        "psr-4": {
            "Bedita\\": "Bedita",
            "Bedita\\Test\\": "Test",
            "": "./Plugin"
        }
    },
    "scripts": {
        "post-install-cmd": "Bedita\\Console\\Installer::postInstall"
    }
}
